        "install_dir": "/usr/local/anyenv",
        "profile_file": "/etc/profile.d/anyenv.sh"
    },
    "perl": {
        "version": "5.20.2"
    },
    "nodejs": {
        "version": "v4.2.2"
    },
    "rakudobrew": {
        "install_dir": "/usr/local/rakudobrew",
        "profile_rc": "/etc/profile.d/rakudobrew.sh"
    },
    "nginx": {
        "config_dir": "/etc/nginx/conf.d",
